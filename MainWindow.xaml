<Window x:Class="CulminatingProblemS2HighTideLowTide.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CulminatingProblemS2HighTideLowTide"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBlock TextWrapping="Wrap">The first line contains the integer N (1 ≤ N ≤ 100). The next line contains N distinct spaceseparated
        positive integers, where each integer is at most 1 000 000.  The output will be the numbers in alternating order.
        </TextBlock>
        <TextBox x:Name="NInput">8</TextBox>
        <TextBox x:Name="TidesInput">10 50 40 7 3 110 90 2</TextBox>
        <Button x:Name="btnStart" Click="btnStart_Click">Click</Button>
        <Label x:Name="lblOutput"></Label>
    </StackPanel>
</Window>
